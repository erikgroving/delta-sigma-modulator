
//i: from 0 to 4194705, 
//initial value: x[0][0]=0;	x[0][1]=0;	x[0][2]=0;	x[0][3]=0;
//u: can get from workspace err;	length(err.data)= 4194705;
//y: should be corresponding with workspace int; length(int.data)= 4194705;

x[i+1][0]=A0[0]*x[i][0]+A0[1]*x[i][1]+A0[2]*x[i][2]+A0[3]*x[i][3]+u[i];
x[i+1][1]=x[i][0];
x[i+1][2]=x[i][1];
x[i+1][3]=x[i][2];
y[i]=C[0]*x[i][0]+C[1]*x[i][1]+C[2]*x[i][2]+C[3]*x[i][3]+D*u[i];
//why the input of quantizer is 32bit while dss_vin_sum_dith is 20bit?
